bind -T copy-mode    MouseDragEnd1Pane send-keys -X copy-pipe-and-cancel "cat | win32yank.exe -i"
bind -T copy-mode    y                 send-keys -X copy-pipe-and-cancel "cat | win32yank.exe -i"
bind -T copy-mode-vi Enter             send-keys -X copy-pipe-and-cancel "cat | win32yank.exe -i"
bind -T copy-mode-vi MouseDragEnd1Pane send-keys -X copy-pipe-and-cancel "cat | win32yank.exe -i"
bind -T copy-mode-vi y                 send-keys -X copy-pipe-and-cancel "cat | win32yank.exe -i"
bind      ] run "cat | win32yank.exe -i | tmux load-buffer - && tmux paste-buffer"
bind     ^] run "cat | win32yank.exe -i | tmux load-buffer - && tmux paste-buffer"
